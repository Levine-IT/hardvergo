{
  "name": "optimize-draft-listing-image",
  "version": "1.0.0",
  "description": "AWS Lambda function to optimize draft listing images triggered by SQS",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "docker:build": "docker build -t optimize-draft-listing-image .",
    "docker:run": "docker run -p 9002:8080 optimize-draft-listing-image"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-sqs": "^3.0.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": ["aws", "lambda", "sqs", "image-optimization"],
